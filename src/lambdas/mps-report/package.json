  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.1",
    "aws-sdk": "^2.1101.0",
    "babel-loader": "^8.2.4",
    "fork-ts-checker-webpack-plugin": "^7.2.1",

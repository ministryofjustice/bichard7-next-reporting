    "csv-stringify": "^6.0.5",
    "jszip": "^3.7.1",
    "pg-promise": "^10.11.1",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.0",
    "aws-sdk": "^2.942.0",
    "babel-loader": "^8.2.3",
    "fork-ts-checker-webpack-plugin": "^7.0.0",
    "jest": "^27.4.7",
    "jest-junit": "^13.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  },
